        "build:web:prod": "ng build --prod",
        "build:native:prod": "ng build native --prod && npm run native:sync",
        "native:sync": "npx cap copy && npx cap update",
        "deploy": "firebase deploy --only hosting",
        "test": "ng test",
        "lint": "nx lint && ng lint",
        "e2e": "ng e2e",
